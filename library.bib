@article{Batygin:2011a,
	Author = {Batygin, Konstantin and Stevenson, David J and Bodenheimer, Peter H},
	Date = {2011/00/19},
	Date-Added = {2014-06-15 21:34:20 +0000},
	Date-Modified = {2014-06-16 01:01:34 +0000},
	Journal = {ArXiv e-prints},
	Keywords = {astro-ph.EP},
