	Author = {Ant{\'o}n, Luis and Zanotti, Olindo and Miralles, Juan A and Mart{\'\i}, Jos{\'e}M and Ib{\'a}{\~n}ez, Jos{\'e}M and Font, Jos{\'e}A and Pons, Jos{\'e}A},
	Date = {2006/00/01},
