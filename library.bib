@article{Jose:2012a,
	Address = {Dept. F{\'\i}sica i Enginyeria Nuclear, EUETIB, Universitat Polit{\`e}cnica de Catalunya, E-08036 Barcelona; Institut d'Estudis Espacials de Catalunya, E-08034 Barcelona, Spain},
	Author = {Jos{\'e}, J},
	Date = {2012/00/01},
