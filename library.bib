@article{Khokhlov:1997a,
	Author = {Khokhlov, A M and Oran, E S and Wheeler, J C},
	Date = {1997/00/01},
	Date-Added = {2014-06-15 21:34:20 +0000},
	Date-Modified = {2014-06-16 01:01:35 +0000},
	Journal = {\apj},
	M3 = {10.1086/303815},
	Month = {00},
	N2 = {We derive the criteria for deflagration to detonation transition (DDT) in a Type Ia supernova. The theory is based on two major assumptions: (1) detonation is triggered via the Zeldovich gradient mechanism inside a region of mixed fuel and products, and (2) the mixed region is produced by a turbulent mixing of fuel and products either inside an active deflagration front or during the global expansion and subsequent contraction of an exploding white dwarf. We determine the critical size of the mixed region required to initiate a detonation in a degenerate carbon-oxygen mixture. This critical length is much larger than the width of the reaction front of a Chapman-Jouguet detonation. However, at densities greater than \~{}=5 x 106 g cm-3, it is much smaller than the size of a white dwarf. We derive the critical turbulent intensity required to create the mixed region inside an active deflagration front in which a detonation can form. We conclude that the density rho tr at which a detonation can form in a carbon-oxygen white dwarf is low, less than 2--5 x 107 g cm-3 but greater than 5 x 106 g cm-3.},
	Pages = {678},
	Title = {Deflagration-to-Detonation Transition in Thermonuclear Supernovae},
	Ty = {JOUR},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=1997ApJ...478..678K&link_type=ABSTRACT},
	Volume = {478},
