@article{Brown:2007a,
	Address = {Smithsonian Astrophysical Observatory, 60 Garden Street, Cambridge, MA 02138 wbrown{\char64}cfa.harvard.edu mkurtz{\char64}cfa.harvard.edu skenyon{\char64}cfa.harvard.edu mgeller{\char64}cfa.harvard.edu},
	Author = {Brown, Warren R and Geller, Margaret J and Kenyon, Scott J and Kurtz, Michael J and Bromley, Benjamin C},
	Date = {2007/00/01},
	Date-Added = {2014-06-15 21:34:21 +0000},
	Date-Modified = {2014-06-16 01:01:34 +0000},
	Journal = {\apj},
	M3 = {10.1086/523642},
	Month = {00},
	N2 = {We report the discovery of three new unbound hypervelocity stars (HVSs), stars traveling with such extreme velocities that dynamical ejection from a massive black hole (MBH) is their only suggested origin. We also detect a population of possibly bound HVSs. The significant asymmetry we observe in the velocity distribution-we find 26 stars with vrf>275 km s-1 and one star with vrf<-275 km s-1-shows that HVSs must be short-lived, probably 3-4 Msolar main-sequence stars. Any population of hypervelocity post-main-sequence stars should contain stars falling back onto the Galaxy, contrary to the observations. The spatial distribution of HVSs also supports the main-sequence interpretation: longer lived 3 Msolar HVSs fill our survey volume; shorter lived 4 Msolar HVSs are missing at faint magnitudes. We infer that there are 96+/-10 HVSs of mass 3-4 Msolar within R<100 kpc, possibly enough HVSs to constrain ejection mechanisms and potential models. Depending on the mass function of HVSs, we predict that SEGUE may find up to 5-15 new HVSs. The travel times of our HVSs favor a continuous ejection process, although a \~{}120 Myr old burst of HVSs is also allowed.},
	Number = {2},
	Pages = {1708--1716},
	Title = {Hypervelocity Stars. III. The Space Density and Ejection History of Main-Sequence Stars from the Galactic Center},
	Ty = {JOUR},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2007ApJ...671.1708B&link_type=ABSTRACT},
	Volume = {671},
	Year = {2007},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2007ApJ...671.1708B&link_type=ABSTRACT}}

@article{Bromley:2012a,
	Address = {Department of Physics \& Astronomy, University of Utah, 115 S 1400 E, Rm 201, Salt Lake City, UT 84112, USA bromley{\char64}physics.utah.edu},
	Author = {Bromley, Benjamin C and Kenyon, Scott J and Geller, Margaret J and Brown, Warren R},
	Date = {2012/00/01},
	Date-Added = {2014-06-15 21:34:21 +0000},
	Date-Modified = {2014-06-16 01:01:34 +0000},
	Journal = {\apjl},
	M3 = {10.1088/2041-8205/749/2/L42},
	Month = {00},
	N2 = {We examine whether disrupted binary stars can fuel black hole growth. In this mechanism, tidal disruption produces a single hypervelocity star (HVS) ejected at high velocity and a former companion star bound to the black hole. After a cluster of bound stars forms, orbital diffusion allows the black hole to accrete stars by tidal disruption at a rate comparable to the capture rate. In the Milky Way, HVSs and the S star cluster imply similar rates of 10-5 to 10-3 yr-1 for binary disruption. These rates are consistent with estimates for the tidal disruption rate in nearby galaxies and imply significant black hole growth from disrupted binaries on 10 Gyr timescales.},
	Number = {2},
	Pages = {L42},
	Title = {Binary Disruption by Massive Black Holes: Hypervelocity Stars, S Stars, and Tidal Disruption Events},
