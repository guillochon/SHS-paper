@article{Wood:2009a,
	Address = {AA(Department of Physics and Space Sciences, Florida Institute of Technology, 150 W. University Blvd., Melbourne, FL 32901, USA; Department of Astrophysics/IMAPP, Radboud University Nijmegen, PO Box 9010, 6500 GL Nijmegen, the Netherlands), AB(Department of Physics and Space Sciences, Florida Institute of Technology, 150 W. University Blvd., Melbourne, FL 32901, USA), AC(NASA KT-C, Kennedy Space Center, FL 32899, USA)},
	Author = {Wood, M A and Thomas, D M and Simpson, J C},
	Date = {2009/00/01},
