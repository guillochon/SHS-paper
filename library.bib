@article{Tielens:1990a,
	Address = {AA(National Aeronautics and Space Administration. Ames Research Center, Moffett Field, CA.)},
	Author = {Tielens, Alexander G G M},
	Date = {1990/00/01},
	Date-Added = {2014-06-15 21:34:20 +0000},
