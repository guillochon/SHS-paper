@article{Schlaufman:2010b,
	Address = {AA(Astronomy and Astrophysics Department, University of California, Santa Cruz, CA 95064, USA ; NSF Graduate Research Fellow.; kcs{\char64}ucolick.org ), AB(Astronomy and Astrophysics Department, University of California, Santa Cruz, CA 95064, USA ; Kavli Institute of Astronomy and Astrophysics, Peking University, Beijing, China ; lin{\char64}ucolick.org ), AC(Tokyo Institute of Technology, Ookayama, Meguro-ku, Tokyo 152-8551, Japan ida{\char64}geo.titech.ac.jp)},
	Author = {Schlaufman, Kevin C and Lin, D N C and Ida, S},
	Date = {2010/00/01},
	Date-Added = {2014-06-15 21:34:20 +0000},
	Date-Modified = {2014-06-16 01:01:35 +0000},
	Journal = {\apjl},
	M3 = {10.1088/2041-8205/724/1/L53},
	Month = {00},
	N2 = {We simulate a Kepler-like observation of a theoretical exoplanet population and show that the observed orbital period distribution of the Kepler giant planet candidates is best matched by an average stellar specific dissipation function Q'* in the interval 106 <\~{} Q'* <\~{} 107. In that situation, the few super-Earths that are driven to orbital periods of P < 1 day by dynamical interactions in multiple-planet systems will survive tidal disruption for a significant fraction of the main-sequence lifetimes of their stellar hosts. Consequently, though these very hot super-Earths are not characteristic of the overall super-Earth population, their substantial transit probability implies that they should be significant contributors to the full super-Earth population uncovered by Kepler. As a result, the CoRoT-7 system may be the first representative of a population of very hot super-Earths that we suggest should be found in multiple-planet systems preferentially orbiting the least-dissipative stellar hosts in the Kepler sample.},
	Pages = {L53},
	Title = {A Population of Very Hot Super-Earths in Multiple-planet Systems Should be Uncovered by Kepler},
	Ty = {JOUR},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2010ApJ...724L..53S&link_type=ABSTRACT},
	Volume = {724},
	Year = {2010},
	Bdsk-Url-1 = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2010ApJ...724L..53S&link_type=ABSTRACT}}

@article{Khokhlov:1989a,
