@article{Shatsky:2002a,
	Address = {Sternberg Astronomical Institute, Universitetskii pr. 13, Moscow 119899, Russia; and Royal Observatory of Belgium, Av. Circulaire 3, Bruxelles 1180, Belgium},
	Author = {Shatsky, N and Tokovinin, A},
	Date = {2002/00/01},
	Date-Added = {2014-06-15 21:34:21 +0000},
