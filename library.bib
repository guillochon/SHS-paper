@article{Belczynski:2005a,
	Address = {AA(New Mexico State University, Department of Astronomy, 1320 Frenger Mall, Las Cruces, NM 88003.; Tombaugh Fellow.), AB(Nicolaus Copernicus Astronomical Center, Bartycka 18, 00-716 Warszawa, Poland; kbelczyn{\char64}nmsu.edu, bulik{\char64}camk.edu.pl, aruiter{\char64}nmsu.edu.), AC(New Mexico State University, Department of Astronomy, 1320 Frenger Mall, Las Cruces, NM 88003.)},
	Author = {Belczynski, Krzysztof and Bulik, Tomasz and Ruiter, Ashley J},
