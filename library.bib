@article{Tremblay:2008a,
	Address = {D{\'e}partement de Physique, Universit{\'e}de Montr{\'e}al, C.P. 6128, Succursale Centreville, Montr{\'e}al, QC H3C 3J7, Canada bergeron{\char64}astro.umontreal.ca tremblay{\char64}astro.umontreal.ca},
	Author = {Tremblay, P E and Bergeron, P},
