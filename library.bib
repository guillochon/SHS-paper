@article{Gezari:2006a,
	Address = {AA(California Institute of Technology, MC 405-47, 1200 East California Boulevard, Pasadena, CA 91125; suvi{\char64}srl.caltech.edu.), AB(California Institute of Technology, MC 405-47, 1200 East California Boulevard, Pasadena, CA 91125; suvi{\char64}srl.caltech.edu.), AC(Laboratoire d'Astrophysique de Marseille, 13376 Marseille Cedex 12, France.), AD(Laboratoire d'Astrophysique de Marseille, 13376 Marseille Cedex 12, France.), AE(Department of Astronomy, Columbia University, New York, NY 10027.), AF(California Institute of ...},
	Author = {Gezari, S and Martin, D C and Milliard, B and Basa, S and Halpern, J P and Forster, K and Friedman, P G and Morrissey, P and Neff, S G and Schiminovich, D and Seibert, M and Small, T and Wyder, T K},
	Date = {2006/00/01},
