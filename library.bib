@article{Armitage:2000a,
	Address = {AA(Current address: Max-Planck-Institut f{\"u}r Astrophysik, Karl-Schwarzschild-Strasse 1, D-85740 Garching, Germany), AB(Space Telescope Science Institute, 3700 San Martin Drive, Baltimore, MD 21218; armitage{\char64}mpa-garching.mpg.de, mlivio{\char64}stsci.edu)},
	Author = {Armitage, Philip J and Livio, Mario},
	Date = {2000/00/01},
	Date-Added = {2014-06-15 21:34:20 +0000},
	Date-Modified = {2014-06-16 01:01:34 +0000},
	Journal = {\apj},
	M3 = {10.1086/308548},
	Month = {00},
	N2 = {Neutron stars inspiralling into a stellar envelope can accrete at rates vastly exceeding the Eddington limit if the flow develops pressures high enough to allow neutrinos to radiate the released gravitational energy. It has been suggested that this hypercritical mode of accretion leads inevitably to the formation of stellar mass black holes, with implications both for some gamma ray burst models and for the predicted event rate in future Laser Interferometer Gravitational-Wave Observatory (LIGO) observations. We study the hydrodynamics of this flow at large radii (R>>Rns), and show that for low Mach number flows, in two dimensions, modest density gradients in the stellar envelope suffice to produce a hot, advection-dominated accretion disk around the accreting object. The fate of the neutron star depends critically on the highly uncertain nature and strength of outflows generated by such a disk. We argue that strong outflows are likely, in which case insufficient accretion occurs to force collapse to a black hole before the envelope has been ejected.},
	Pages = {540},
	Title = {Black Hole Formation via Hypercritical Accretion during Common-Envelope Evolution},
	Ty = {JOUR},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2000ApJ...532..540A&link_type=ABSTRACT},
	Volume = {532},
	Year = {2000},
