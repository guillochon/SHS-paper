@article{Kerschbaum:2010a,
	Address = {University of Vienna, Department of Astronomy, T{\"u}rkenschanzstrasse 17, 1180 Vienna, Austria franz.kerschbaum{\char64}univie.ac.at},
	Author = {Kerschbaum, F and Lebzelter, T and Mekul, L},
	Date = {2010/00/01},
