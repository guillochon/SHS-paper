@article{Hubbard:1984a,
	Address = {AA(Arizona, University, Tucson, AZ)},
	Author = {Hubbard, W B},
	Date = {1984/00/01},
	Date-Added = {2014-06-15 21:34:20 +0000},
	Date-Modified = {2014-06-16 01:01:35 +0000},
	Journal = {New York},
	Month = {00},
	N2 = {Known processes and current theories on planetary structure are analyzed in this book. Data obtained over the past decade by both deep-space missions and earthbound observers are examined, and mathematics-assisted explanations of the dynamics and chemistry of each planet in the solar system except Pluto are given. A comprehensive scheme for understanding the evolution of the planets is explored. Planetary thermodynamics is discussed, including constituent relationships for high pressures and temperatures. The behavior of hydrogen, helium, water, iron, rock, and other materials during the evolution of planetary interiors is addressed. Models of heat flow and gravitational potentials are used to demonstrate the probable characteristics of planetary interiors. The roles of rapidly rotating Maclaurin spheroids, hydrostatic equilibrium, tidal torques, and tidal heating in planetary structures are explained. The mechanisms of heat transport, planetary energy sources and their distribution, characteristics of planetary magnetism, and differences between terrestrial and Jovian planets are discussed.},
	Title = {Planetary interiors},
	Ty = {JOUR},
