@article{Timmes:2003a,
	Address = {AA(Center for Astrophysical Thermonuclear Flashes, University of Chicago, 5640 South Ellis Avenue, Chicago, IL 60637; fxt{\char64}burn.uchicago.edu.; Department of Astronomy and Astrophysics, University of Chicago, 5640 South Ellis Avenue, Chicago, IL 60637.), AB(Center for Astrophysical Thermonuclear Flashes, University of Chicago, 5640 South Ellis Avenue, Chicago, IL 60637; fxt{\char64}burn.uchicago.edu.; Enrico Fermi Institute, University of Chicago, 5640 South Ellis Avenue, Chicago, IL 60637.), AC(Center for Astrophysi...},
	Author = {Timmes, F X and Brown, Edward F and Truran, J W},
	Date = {2003/00/01},
	Date-Added = {2014-06-15 21:34:20 +0000},
	Date-Modified = {2014-06-16 01:01:35 +0000},
	Journal = {\apj},
	M3 = {10.1086/376721},
	Month = {00},
	N2 = {We explore the idea that the observed variations in the peak luminosities of Type Ia supernovae (SNe Ia) originate in part from a scatter in metallicity of the main-sequence stars that become white dwarfs. Previous numerical studies have not self-consistently explored metallicities greater than solar. One-dimensional Chandrasekhar mass models of SNe Ia produce most of their 56Ni in a burn to nuclear statistical equilibrium between the mass shells 0.2 and 0.8 Msolar, for which the electron-to-nucleon ratio Ye is constant during the burn. We show analytically that under these conditions, charge and mass conservation constrain the mass of 56Ni produced to depend linearly on the original metallicity of the white dwarf progenitor. Detailed postprocessing of W7-like models confirms this linear dependence. The effect that we have identified is most evident at metallicities larger than solar and is in agreement with previous self-consistent calculations over the metallicity range common to both calculations. The observed scatter in the metallicity (1/3-3 Zsolar) of the solar neighborhood is enough to induce a 25{\%} variation in the mass of 56Ni ejected by SNe Ia. This is sufficient to vary the peak V-band brightness by {$|$}ΔMV{$|$}\~{}0.2. This scatter in metallicity is present out to the limiting redshifts of current observations (z<\~{}1). Sedimentation of 22Ne can possibly amplify the variation in 56Ni mass to <\~{}50{\%}. Further numerical studies can determine if other metallicity-induced effects, such as a change in the mass of the 56Ni-producing region, offset or enhance the variation that we identify.},
	Pages = {L83},
	Title = {On Variations in the Peak Luminosity of Type Ia Supernovae},
	Ty = {JOUR},
	Url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?db=pubmed&cmd=Retrieve&dopt=AbstractPlus&list_uids=2003ApJ...590L..83T},
	Volume = {590},
	Year = {2003},
	Bdsk-File-1 = {},
