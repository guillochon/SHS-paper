@article{De-Colle:2006a,
	Address = {Instituto de Astronom{\'\i}a, Universidad Nacional Aut{\'o}noma de M{\'e}xico, Ap.P. 70264, 04510 DF, Mexico fdecolle{\char64}astroscu.unam.mx},
	Author = {De Colle, Fabio and Raga, A C},
	Date = {2006/00/01},
