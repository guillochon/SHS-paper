@article{Goobar:2002a,
	Address = {Department of Physics, Stockholm University, SCFAB, 106 91 Stockholm, Sweden},
	Author = {Goobar, A and Bergstr{\"o}m, L and M{\"o}rtsell, E},
	Date = {2002/00/01},
	Date-Added = {2014-06-15 21:34:20 +0000},
