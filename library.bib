@article{Belczynski:2008a,
	Address = {AA(Department of Astronomy, New Mexico State University, 1320 Frenger Mall, Las Cruces, NM 88003; kbelczyn{\char64}nmsu.edu.; Tombaugh Fellow.), AB(Department of Physics and Astronomy, Northwestern University, 2145 Sheridan Road, Evanston, IL 60208; vicky{\char64}northwestern.edu, rasio{\char64}northwestern.edu, r-taam{\char64}northwestern.edu.), AC(Department of Physics and Astronomy, Northwestern University, 2145 Sheridan Road, Evanston, IL 60208; vicky{\char64}northwestern.edu, rasio{\char64}northwestern.edu, r-taam{\char64}northwestern.edu.), AD(Department o...},
	Author = {Belczynski, Krzysztof and Kalogera, Vassiliki and Rasio, Frederic A and Taam, Ronald E and Zezas, Andreas and Bulik, Tomasz and Maccarone, Thomas J and Ivanova, Natalia},
	Date = {2008/00/01},
