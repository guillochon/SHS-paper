	Title = {Ion-by-ion Cooling Efficiencies},
	Ty = {JOUR},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2012ApJS..199...20G&link_type=ABSTRACT},
	Volume = {199},
	Year = {2012},
	Bdsk-File-1 = {},
