@article{Ballone:2013a,
	Address = {University Observatory Munich, Scheinerstra{\ss}e 1, D-81679 M{\"u}nchen, Germany ; Max-Planck-Institute for Extraterrestrial Physics, Postfach 1312, Giessenbachstra{\ss}e, D-85741 Garching, Germany; aballone{\char64}mpe.mpg.de},
	Author = {Ballone, A and Schartmann, M and Burkert, A and Gillessen, S and Genzel, R and Fritz, T K and Eisenhauer, F and Pfuhl, O and Ott, T},
	Date = {2013/00/01},
	Date-Added = {2014-06-15 21:34:21 +0000},
	Date-Modified = {2014-06-16 01:01:34 +0000},
	Journal = {\apj},
	M3 = {10.1088/0004-637X/776/1/13},
	Month = {00},
	N2 = {The origin of the dense gas cloud G2 discovered in the Galactic Center is still a debated puzzle. G2 might be a diffuse cloud or the result of an outflow from an invisible star embedded in it. We present hydrodynamical simulations of the evolution of different spherically symmetric winds of a stellar object embedded in G2. We find that the interaction with the ambient medium and the extreme gravitational field of the supermassive black hole in the Galactic Center must be taken into account in such a source scenario. The thermal pressure of the hot and dense atmosphere confines the wind, while its ram pressure shapes it via stripping along the orbit, with the details depending on the wind parameters. Tidal forces squeeze the wind near pericenter, reducing it to a thin and elongated filament. We also find that in this scenario most of the Brγluminosity is expected to come from the densest part of the wind, which has a highly filamentary structure with a low filling factor. For our assumed atmosphere, the observations can be best matched by a mass outflow rate of \dot{\{}M{\}}{\{}{\_}w{\}}=8.8\times 10\^{}{\{}-8{\}}\, M{\_}{\{}\odot {\}} \;yr\^{}{\{}-1{\}} and a wind velocity of v w = 50 km s-1. These values are comparable with those of a young T Tauri star wind, as already suggested by Scoville \& Burkert.},
	Number = {1},
	Pages = {13},
	Title = {Hydrodynamical Simulations of a Compact Source Scenario for the Galactic Center Cloud G2},
	Ty = {JOUR},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2013ApJ...776...13B&link_type=ABSTRACT},
	Volume = {776},
	Year = {2013},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2013ApJ...776...13B&link_type=ABSTRACT}}

@article{Phifer:2013a,
	Address = {Department of Physics and Astronomy, University of California, Los Angeles, CA 90095, USA ; NSF Graduate Student Fellow, Grant DGE-0707424.; ghez{\char64}astro.ucla.edu},
	Author = {Phifer, K and Do, T and Meyer, L and Ghez, A M and Witzel, G and Yelda, S and Boehle, A and Lu, J R and Morris, M R and Becklin, E E and Matthews, K},
	Date = {2013/00/01},
	Date-Added = {2014-06-15 21:34:21 +0000},
	Date-Modified = {2014-06-16 01:01:35 +0000},
	Journal = {\apjl},
	M3 = {10.1088/2041-8205/773/1/L13},
	Month = {00},
	N2 = {We present new observations and analysis of G2---the intriguing red emission-line object which is quickly approaching the Galaxy's central black hole. The observations were obtained with the laser guide star adaptive optics systems on the W. M. Keck I and II telescopes (2006-2012) and include spectroscopy (R \~{} 3600) centered on the hydrogen Brγline as well as K' (2.1 μm) and L' (3.8 μm) imaging. Analysis of these observations shows the Brγline emission has a positional offset from the L' continuum. This offset is likely due to background source confusion at L'. We therefore present the first orbital solution derived from Brγline astrometry, which, when coupled with radial velocity measurements, results in a later time of closest approach (2014.21 $\pm$0.14), closer periastron (130 AU, 1600 R s), and higher eccentricity (0.9814 $\pm$0.0060) compared to a solution using L' astrometry. It is shown that G2 has no K' counterpart down to K' \~{} 20 mag. G2's L' continuum and the Brγline emission appears unresolved in almost all epochs, which implies that the bulk of the emission resides in a compact region. The observations altogether suggest that while G2 has a gaseous component that is tidally interacting with the central black hole, there is likely a central star providing the self-gravity necessary to sustain the compact nature of this object.},
	Number = {1},
	Pages = {L13},
	Title = {Keck Observations of the Galactic Center Source G2: Gas Cloud or Star?},
	Ty = {JOUR},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2013ApJ...773L..13P&link_type=ABSTRACT},
	Volume = {773},
	Year = {2013},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2013ApJ...773L..13P&link_type=ABSTRACT}}

@article{Dyson:1979a,
	Address = {University College, Cardiff, Wales},
	Author = {Dyson, J. and Schutz, B. F.},
	Date = {1979/00/01},
	Date-Added = {2014-06-15 21:34:21 +0000},
	Date-Modified = {2014-06-16 01:01:35 +0000},
	Journal = {Royal Society (London)},
	M3 = {10.1098/rspa.1979.0137},
	Month = {00},
	N2 = {It is noted that linear adiabatic perturbations of a differentially rotating, axisymmetric, perfect fluid stellar model have normal modes described by a quadratic problem. The paper studies the problem and the associated time evolution equation. It is shown that in the Hilbert space H-prime, whose norm is square-integration weighted by A, the operators (A to the -1st power)(B) and (A to the -1st power)(C) are anti-selfadjoint and selfadjoint, respectively, when restricted to vectors belonging to a particular but arbitrary axial harmonic. Bounds are found on the spectrum of normal modes and it is shown that any initial data in the domain of C leads to a solution whose growth rate is limited by the spectrum and which can be expressed in a certain weak sense as a linear superposition of the normal modes.},
	Pages = {389--410},
	Title = {Perturbations and stability of rotating stars. I - Completeness of normal modes},
	Ty = {JOUR},
