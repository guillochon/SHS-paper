@article{Pancoast:2012a,
	Address = {Department of Physics, University of California, Santa Barbara, CA 93106, USA pancoast{\char64}physics.ucsb.edu},
	Author = {Pancoast, Anna and Brewer, Brendon J and Treu, Tommaso and Barth, Aaron J and Bennert, Vardha N and Canalizo, Gabriela and Filippenko, Alexei V and Gates, Elinor L and Greene, Jenny E and Li, Weidong and Malkan, Matthew A and Sand, David J and Stern, Daniel and Woo, Jong-Hak and Assef, Roberto J and Bae, Hyun-Jin and Buehler, Tabitha and Cenko, S Bradley and Clubb, Kelsey I and Cooper, Michael C and Diamond-Stanic, Aleksandar M and Hiner, Kyle D and H{\"o}nig, Sebastian F and Joner, Michael D and Kandrashoff, Michael T and Laney, C David and Lazarova, Mariana S and Nierenberg, A M and Park, Dawoo and Silverman, Jeffrey M and Son, Donghoon and Sonnenfeld, Alessandro and Thorman, Shawn J and Tollerud, Erik J and Walsh, Jonelle L and Walters, Richard},
	Date = {2012/00/01},
	Date-Added = {2014-06-15 21:34:20 +0000},
	Date-Modified = {2014-06-16 01:01:35 +0000},
	Journal = {\apj},
	M3 = {10.1088/0004-637X/754/1/49},
	Month = {00},
	N2 = {We present dynamical modeling of the broad-line region (BLR) in the Seyfert 1 galaxy Mrk 50 using reverberation mapping data taken as part of the Lick AGN Monitoring Project (LAMP) 2011. We model the reverberation mapping data directly, constraining the geometry and kinematics of the BLR, as well as deriving a black hole mass estimate that does not depend on a normalizing factor or virial coefficient. We find that the geometry of the BLR in Mrk 50 is a nearly face-on thick disk, with a mean radius of 9.6+1.2 -0.9 light days, a width of the BLR of 6.9+1.2 -1.1 light days, and a disk opening angle of 25 $\pm$10 deg above the plane. We also constrain the inclination angle to be 9+7 -5 deg, close to face-on. Finally, the black hole mass of Mrk 50 is inferred to be log10(M BH/M \&sun;) = 7.57+0.44 -0.27. By comparison to the virial black hole mass estimate from traditional reverberation mapping analysis, we find the normalizing constant (virial coefficient) to be log10 f = 0.78+0.44 -0.27, consistent with the commonly adopted mean value of 0.74 based on aligning the M BH-σ* relation for active galactic nuclei and quiescent galaxies. While our dynamical model includes the possibility of a net inflow or outflow in the BLR, we cannot distinguish between these two scenarios.},
	Number = {1},
	Pages = {49},
	Title = {The Lick AGN Monitoring Project 2011: Dynamical Modeling of the Broad-line Region in Mrk 50},
	Ty = {JOUR},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2012ApJ...754...49P&link_type=ABSTRACT},
	Volume = {754},
