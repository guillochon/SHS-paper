@article{Bryan:1995a,
	Author = {Bryan, Greg L and Norman, Michael L and Stone, James M and Cen, Renyue and Ostriker, Jeremiah P},
	Date = {1995/00/01},
	Date-Added = {2014-06-15 21:34:20 +0000},
