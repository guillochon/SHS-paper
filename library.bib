@article{Syer:1999a,
	Address = {AA(Max-Planck-Institut f{\"u}r Astrophysik, Karl-Schwarzschild-Strasse 1, 85748 Garching bei M{\"u}nchen, Germany), AB(Max-Planck-Institut f{\"u}r Astrophysik, Karl-Schwarzschild-Strasse 1, 85748 Garching bei M{\"u}nchen, Germany)},
