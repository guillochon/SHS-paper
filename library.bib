@article{Campana:2011a,
	Address = {INAF - Osservatorio Astronomico di Brera, Via E. Bianchi 46, I-23807 Merate (LC), Italy},
	Author = {Campana, S and Lodato, G and D'avanzo, P and Panagia, N and Rossi, E M and Della Valle, M and Tagliaferri, G and Antonelli, L A and Covino, S and Ghirlanda, G and Ghisellini, G and Melandri, A and Pian, E and Salvaterra, R and Cusumano, G and D'elia, V and Fugazza, D and Palazzi, E and Sbarufatti, B and Vergani, D S},
	Date = {2011/00/01},
	Date-Added = {2014-06-15 21:34:21 +0000},
	Date-Modified = {2014-06-16 01:01:34 +0000},
	Journal = {\nat},
	M3 = {10.1038/nature10592},
	Month = {00},
	N2 = {The tidal disruption of a solar-mass star around a supermassive black hole has been extensively studied analytically and numerically. In these events, the star develops into an elongated banana-shaped structure. After completing an eccentric orbit, the bound debris falls into the black hole, forming an accretion disk and emitting radiation. The same process may occur on planetary scales if a minor body passes too close to its star. In the Solar System, comets fall directly into our Sun or onto planets. If the star is a compact object, the minor body can become tidally disrupted. Indeed, one of the first mechanisms invoked to produce strong gamma-ray emission involved accretion of comets onto neutron stars in our Galaxy. Here we report that the peculiarities of the `Christmas' gamma-ray burst (GRB 101225A) can be explained by a tidal disruption event of a minor body around an isolated Galactic neutron star. This would indicate either that minor bodies can be captured by compact stellar remnants more frequently than occurs in the Solar System or that minor-body formation is relatively easy around millisecond radio pulsars. A peculiar supernova associated with a gamma-ray burst provides an alternative explanation.},
	Number = {7},
	Pages = {69--71},
	Publisher = {Cornell University Library},
	Title = {The unusual gamma-ray burst GRB 101225A explained as a minor body falling onto a neutron star},
	Ty = {JOUR},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2011Natur.480...69C&link_type=ABSTRACT},
	Volume = {480},
	Year = {2011},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2011Natur.480...69C&link_type=ABSTRACT}}

