@article{Terebey:1998a,
	Author = {Terebey, S and van Buren, D and Padgett, D L and Hancock, T and Brundage, M},
	Date = {1998/00/31},
	Date-Added = {2014-06-15 21:34:20 +0000},
	Date-Modified = {2014-06-16 01:01:35 +0000},
