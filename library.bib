@article{Loren-Aguilar:2010a,
	Address = {Departament de F{\'\i}sica Aplicada, Universitat Polit{\`e}cnica de Catalunya, c/Esteve Terrades 5, 08860 Castelldefels, Spain; Institute for Space Studies of Catalonia, c/Gran Capit{\`a}2-4, Edif. Nexus 104, 08034 Barcelona, Spain},
	Author = {Lor{\'e}n-Aguilar, P and Isern, J and Garc{\'\i}a-Berro, E},
	Date = {2010/00/01},
	Date-Added = {2014-06-15 21:34:21 +0000},
