@article{Magorrian:1999a,
	Address = {AA(CITA, University of Toronto, 60 St George Street, Toronto, Ontario, Canada M5S 3H8; Institute of Astronomy, Madingley Road, Cambridge CB3 0HA), AB(Princeton University Observatory, Peyton Hall, Princeton, NJ 08544, USA)},
	Author = {Magorrian, John and Tremaine, Scott},
	Date = {1999/00/01},
	Date-Added = {2014-06-15 21:34:20 +0000},
	Date-Modified = {2014-06-16 01:01:35 +0000},
	Journal = {\mnras},
	Month = {00},
	N2 = {There is strong evidence for some kind of massive dark object in the centres of many galaxy bulges. The detection of flares from tidally disrupted stars could confirm that these objects are black holes (BHs). Here we present calculations of the stellar disruption rates in detailed dynamical models of real galaxies, taking into account the refilling of the loss cone of stars on disruptable orbits by two-body relaxation and tidal forces in non-spherical galaxies. The highest disruption rates (one star per 104yr) occur in faint (L<\~{}1010Lsolar) galaxies, which have steep central density cusps. More luminous galaxies are less dense and have much longer relaxation times and more massive BHs. Dwarf stars in such galaxies are swallowed whole by the BH and hence do not emit flares; giant stars could produce flares as often as every 105yr, although the rate depends sensitively on the shape of the stellar distribution function. We discuss the possibility of detecting disruption flares in current supernova searches. The total mass of stars consumed over the lifetime of the galaxy is of the order of 106Msolar, independent of galaxy luminosity; thus, disrupted stars may contribute significantly to the present BH mass in galaxies fainter than \~{}109Lsolar.},
	Pages = {447},
	Title = {Rates of tidal disruption of stars by massive central black holes},
	Ty = {JOUR},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=1999MNRAS.309..447M&link_type=ABSTRACT},
	Volume = {309},
	Year = {1999},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=1999MNRAS.309..447M&link_type=ABSTRACT}}

@article{Li:2002a,
	Address = {AA(Harvard-Smithsonian Center for Astrophysics, 60 Garden Street, Cambridge, MA 02138; lli{\char64}cfa.harvard.edu, rnarayan{\char64}cfa.harvard.edu.; Chandra Fellow.), AB(Harvard-Smithsonian Center for Astrophysics, 60 Garden Street, Cambridge, MA 02138; lli{\char64}cfa.harvard.edu, rnarayan{\char64}cfa.harvard.edu.), AC(Chandra Fellow.; Princeton University Observatory, Princeton, NJ 08544; kristen{\char64}astro.princeton.edu.)},
	Author = {Li, Li-Xin and Narayan, Ramesh and Menou, Kristen},
	Date = {2002/00/01},
	Date-Added = {2014-06-15 21:34:20 +0000},
	Date-Modified = {2014-06-16 01:01:35 +0000},
	Journal = {\apj},
	M3 = {10.1086/341890},
	Month = {00},
	N2 = {We model the 1990 giant X-ray flare of the quiescent galaxy NGC 5905 as the tidal disruption of a star by a supermassive black hole. From the observed rapid decline of the luminosity, over a timescale of a few years, we argue that the flare was powered by the fallback of debris rather than subsequent accretion via a thin disk. The fallback model allows constraints to be set on the black hole mass and the mass of debris. The latter must be very much less than a solar mass to explain the very low luminosity of the flare. The observations can be explained either as the partial stripping of the outer layers of a low-mass main-sequence star or as the disruption of a brown dwarf or a giant planet. We find that the X-ray emission in the flare must have originated within a small patch rather than over the entire torus of circularized material surrounding the black hole. We suggest that the patch corresponds to the ``bright spot'' where the stream of returning debris impacts the torus. Interestingly, although the peak luminosity of the flare was highly sub-Eddington, the peak flux from the bright spot was close to the Eddington limit. We speculate on the implications of this result for observations of other flare events.},
	Pages = {753},
	Title = {The Giant X-Ray Flare of NGC 5905: Tidal Disruption of a Star, a Brown Dwarf, or a Planet?},
	Ty = {JOUR},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2002ApJ...576..753L&link_type=ABSTRACT},
	Volume = {576},
	Year = {2002},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2002ApJ...576..753L&link_type=ABSTRACT}}

