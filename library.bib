@article{Pessah:2003a,
	Address = {AA(Steward Observatory, University of Arizona, 933 North Cherry Avenue, Tucson, AZ 85721.), AB(Steward Observatory, University of Arizona, 933 North Cherry Avenue, Tucson, AZ 85721.; Department of Physics, University of Arizona, Tucson, AZ 85721.; Sir Thomas Lyle Fellow and Miegunyah Fellow.)},
	Author = {Pessah, Martin and Melia, Fulvio},
	Date = {2003/00/01},
	Date-Added = {2014-06-15 21:34:20 +0000},
	Date-Modified = {2014-06-16 01:01:35 +0000},
	Journal = {\apj},
	M3 = {10.1086/373942},
	Month = {00},
	N2 = {There is now compelling evidence for the presence of a few million solar masses of dark matter within 0.015 pc of the nonthermal radio source Sgr A*. Recent Chandra observations suggest that the X-ray emission from this source is partially diffuse. This result provides an important clue that can be used to set some constraints on the mass distribution surrounding the black hole, and we here develop a simple model in which the diffuse emission is produced by a halo of neutron stars accreting from the gas falling toward the center. We discuss the various accretion mechanisms that are likely to contribute significantly to the X-ray flux and show that a highly magnetized fraction of old neutron stars may account for the diffuse high-energy source. If this picture is correct, the upper bound to the mass of the central black hole is \~{}2.2×106 Msolar. The core radius of the dark cluster must then be \~{}0.06 pc. We also discuss the sensitivity of our results to the various assumptions made in our calculations.},
	Pages = {L29},
	Title = {Diffuse X-Rays from a Distributed Component of Dark Matter Surrounding Sagittarius A*},
	Ty = {JOUR},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2003ApJ...585L..29P&link_type=ABSTRACT},
