	Year = {1998},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=1998Natur.395A..14R&link_type=ABSTRACT}}

@article{Soltan:1982a,
	Address = {Polska Akademia Nauk, Centrum Astronomiczne, Warsaw, Poland},
	Author = {Soltan, A},
	Date = {1982/00/01},
	Date-Added = {2014-06-15 21:34:21 +0000},
	Date-Modified = {2014-06-16 01:01:35 +0000},
	Journal = {\mnras},
	Month = {00},
	N2 = {Quasar masses are investigated assuming that accretion on to massive black holes is the ultimate source of energy produced by quasars. Lower limit for the total energy emitted and the mass accumulated in black holes in one cubic Gpc is calculated using various data on quasar counts and bolometric luminosities. The energy produced is at least 8.5 x 10 to the 66th erg/cubic Gpc. This result is independent of the cosmological model. Assuming that quasars reside in nuclei of giant galaxies it is shown that minimum masses of dead quasars are of the order of 100 million solar masses, close to the observational threshold for ground-based telescopes.},
	Pages = {115--122},
	Title = {Masses of quasars},
	Ty = {JOUR},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=1982MNRAS.200..115S&link_type=ABSTRACT},
	Volume = {200},
	Year = {1982},
	Bdsk-File-1 = {},
