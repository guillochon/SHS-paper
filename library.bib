@article{Hao:2010a,
	Address = {Harvard-Smithsonian Center for Astrophysics, 60 Garden Street, Cambridge, MA 02138, USA hhao{\char64}cfa.harvard.edu},
	Author = {Hao, Heng and Elvis, Martin and Civano, Francesca and Lanzuisi, Giorgio and Brusa, Marcella and Lusso, Elisabeta and Zamorani, Gianni and Comastri, Andrea and Bongiorno, Angela and Impey, Chris D and Koekemoer, Anton M and Le Floc'h, Emeric and Salvato, Mara and Sanders, David and Trump, Jonathan R and Vignali, Cristian},
	Date = {2010/00/01},
