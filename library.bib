@article{Pinto:2000a,
	Address = {Steward Observatory, University of Arizona Tucson, AZ 85721 ppinto{\char64}as.arizona.edu},
	Author = {Pinto, Philip A and Eastman, Ronald G},
	Date = {2000/00/01},
	Date-Added = {2014-06-15 21:34:21 +0000},
	Date-Modified = {2014-06-16 01:01:35 +0000},
	Journal = {\apj},
	M3 = {10.1086/308380},
	Month = {00},
	N2 = {We examine the nature of the opacity and radiation transport in Type Ia supernovae. The dominant opacity arises from line transitions. We discuss the nature of line opacities and diffusion in expanding media and the appropriateness of various mean and expansion opacities used in light-curve calculations. Fluorescence is shown to be the dominant physical process governing the rate at which energy escapes the supernova. We present a sample light curve that was obtained using a time-dependent solution of the radiative transport equation with a spectral resolution of 80 km s-1 and employing an LTE equation of state. The result compares favorably with light curves and spectra of typical supernovae and is used to illustrate the physics controlling the evolution of the light curve and especially the secondary maxima seen in infrared photometry.},
	Number = {2},
	Pages = {757--776},
	Title = {The Physics of Type IA Supernova Light Curves. II. Opacity and Diffusion},
	Ty = {JOUR},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2000ApJ...530..757P&link_type=ABSTRACT},
	Volume = {530},
