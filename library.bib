@article{Sweigart:1990a,
	Address = {AA(NASA, Goddard Space Flight Center, Greenbelt, MD), AB(Bologna, Universita, Italy), AC(Bologna, Universita, Italy)},
	Author = {Sweigart, Allen V and Greggio, Laura and Renzini, Alvio},
