@article{Narayan:1998a,
	Author = {Narayan, R and Mahadevan, R and Grindlay, J E and Popham, R G and Gammie, C},
	Date = {1998/00/01},
	Date-Added = {2014-06-15 21:34:21 +0000},
	Date-Modified = {2014-06-16 01:01:35 +0000},
	Journal = {\apj},
	M3 = {10.1086/305070},
	Month = {00},
	N2 = {Sagittarius A*, which is located at the Galactic center, is a puzzling source. It has a mass of (2.5$\pm$0.4)×106 Msun, which makes it an excellent black hole candidate. Observations of stellar winds and other gas flows in its vicinity suggest a mass accretion rate (M˙) of a few times 10-6 Msunyr-1. However, such an accretion rate would imply a luminosity greater than 1040 erg s-1 if the radiative efficiency is the usual 10{\%}, whereas observations indicate a bolometric luminosity less than 1037 erg s-1. The spectrum of Sgr A* is unusual, with emission extending over many decades of wavelength. The authors present a model of Sgr A* that is based on a two temperature optically thin advection-dominated accretion flow. The model is consistent with the estimated M and M˙and fits the observed fluxes in the cm/mm and X-ray bands, as well as upper limits in the submm and infrared bands; the fit is less good in the radio spectrum below 86 GHz and in γ-rays above 100 MeV. The very low luminosity of Sgr A* is explained naturally in the model by means of advection. Most of the viscously dissipated energy is advected into the central mass by the accreting gas, and therefore the radiative efficiency is extremely low, ≡5×10-6. A critical element of the model is the presence of an event horizon at the center that swallows the advected energy.},
	Pages = {554--568},
	Title = {Advection-dominated accretion model of Sagittarius A*: evidence for a black hole at the Galactic center.},
	Ty = {JOUR},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=1998ApJ...492..554N&link_type=ABSTRACT},
