@article{Arras:2009a,
	Annote = {3 pages, no figures},
	Author = {Arras, Phil and Socrates, Aristotle},
	Date = {2009/00/01},
	Date-Added = {2014-06-15 21:34:21 +0000},
	Date-Modified = {2014-06-16 01:01:34 +0000},
	Journal = {ArXiv e-prints},
	Month = {00},
	N2 = {Motivated by the comments of Goodman (2009) on our paper concerning thermal tides (Arras and Socrates 2009a), we have studied an idealized problem to understand the global response of a completely fluid gas giant planet to thermal forcing at the surface (Arras and Socrates 2009b). Our findings disagree with the main claims in Goodman (2009). We find that significant quadrupole moments can indeed be induced as a result of thermal forcing. Furthermore, we find that it is possible for the orientation of the quadrupoles to be such that the planet is torqued away from synchronous rotation. Given these results, we believe our proposed thermal tide mechanism (Arras and Socrates 2009a) provides a viable scenario for generating steady-state asychronous rotation, inflated radii and possibly eccentric orbits of the hot Jupiters.},
	Pages = {2318},
	Publisher = {Cornell University Library},
	Title = {Response to "Concerning Thermal Tides on Hot Jupiters" (Goodman 2009; arXiv: 0901.3279)},
	Ty = {JOUR},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2009arXiv0912.2318A&link_type=ABSTRACT},
	Volume = {0912},
	Year = {2009},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2009arXiv0912.2318A&link_type=ABSTRACT}}

@article{Goodman:2009a,
	Annote = {Research note, not for publication. 2 pages, 1 figure},
	Author = {Goodman, Jeremy},
	Date = {2009/00/01},
	Date-Added = {2014-06-15 21:34:21 +0000},
	Date-Modified = {2014-06-16 01:01:35 +0000},
	Journal = {ArXiv e-prints},
	Month = {00},
	N2 = {By analogy with a mechanism proposed by Gold and Soter to explain the retrograde rotation of Venus, Arras and Socrates suggest that thermal tides may excite hot jovian exoplanets into nonsynchronous rotation, and perhaps also noncircular orbits. It is shown here that because of the absence of a solid surface above the convective core of a jovian planet, the coupling of the gravitational and thermal tides vanishes to zeroth order in the ratio of the atmospheric scale height to the planetary radius. At the next order, the effect probably has the sign opposite to that claimed by the latter authors, hence reinforcing synchronous and circular orbits.},
	Pages = {3279},
	Publisher = {Cornell University Library},
	Title = {Concerning thermal tides on hot Jupiters},
	Ty = {JOUR},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2009arXiv0901.3279G&link_type=ABSTRACT},
