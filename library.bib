@article{Nelemans:2001a,
	Address = {AA(Astronomical Institute ``Anton Pannekoek'', Kruislaan 403, 1098 SJ Amsterdam, The Netherlands), AB(Massachusetts Institute of Technology, Massachusetts Ave. 77, Cambridge MA 02139, USA), AC(Astronomical Institute, Utrecht University, PO Box 80000, 3508 TA Utrecht, The Netherlands), AD(Astronomical Institute ``Anton Pannekoek'', Kruislaan 403, 1098 SJ Amsterdam, The Netherlands; Institute of Astronomy of the Russian Academy of Sciences, 48 Pyatnitskaya Str., 109017 Moscow, Russia)},
	Author = {Nelemans, G and Portegies Zwart, S F and Verbunt, F and Yungelson, L R},
	Date = {2001/00/28},
